<%- include('header'); -%>
<body>
  <div class="page-header text-center">
    <h1 class="feedTitle"><span class="fa fa-camera-retro"></span> Feed</h1>
    <a class="btn btn-default btn-sm" href="/profile">Profile</a>
    <a href="/logout" class="btn btn-default btn-sm">Logout</a>
  </div>
  <ul class="feedUl">
  <% for(var i=0; i<posts.length; i++) {%>
    <li class="liFeed">
      <span><a class="user" href="/profile/<%= posts[i].posterId%>">user</span>
      <a href="/post/<%= posts[i]._id%>">
        <img src="<%= posts[i].imgPath%>">
      </a>
      <span class="cap"><%- posts[i].caption %></span>
      <!-- <span><%= posts[i].likes %></span> -->
      <!-- <span><i class="fa fa-heart" aria-hidden="true"></i></span>
      <span><i class="fa fa-trash" aria-hidden="true"></i></span> -->
    </li>
  <% } %>
  </ul>
</body>
<%- include('footer'); -%>
